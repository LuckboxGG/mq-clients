  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@luckbox/eslint-rules": "^5.0.1",
    "@types/amqplib": "^0.8.2",
    "@types/core-js": "^2.5.2",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.155",
    "@types/p-queue": "^3.2.1",
    "@typescript-eslint/parser": "^2.17.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.0.10",
    "jest": "^29.2.1",
    "lint-staged": "^10.0.1",
    "lodash": "^4.17.20",
    "ts-essentials": "^6.0.5",
    "ts-jest": "^29.0.3",
    "tsc-watch": "^4.1.0",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
